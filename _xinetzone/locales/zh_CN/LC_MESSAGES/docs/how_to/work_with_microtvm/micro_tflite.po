msgid ".. code-block:: bash"
msgstr ""

#: ../../../xin/docs/how_to/work_with_microtvm/micro_tflite.ipynb:30029
msgid "pip install tflite=2.1.0 --user"
msgstr ""

#: ../../../xin/docs/how_to/work_with_microtvm/micro_tflite.ipynb:30031
msgid "Generate the TFLite package yourself. The steps are the following:"
msgstr ""

#: ../../../xin/docs/how_to/work_with_microtvm/micro_tflite.ipynb:30033
msgid ""
"Get the flatc compiler.  Please refer to "
"https://github.com/google/flatbuffers for details  and make sure it is "
"properly installed."
msgstr ""

#: ../../../xin/docs/how_to/work_with_microtvm/micro_tflite.ipynb:30039
msgid "flatc --version"
msgstr ""

#: ../../../xin/docs/how_to/work_with_microtvm/micro_tflite.ipynb:30041
msgid "Get the TFLite schema."
msgstr ""

#: ../../../xin/docs/how_to/work_with_microtvm/micro_tflite.ipynb:30045
msgid ""
"wget "
"https://raw.githubusercontent.com/tensorflow/tensorflow/r1.13/tensorflow/lite/schema/schema.fbs"
msgstr ""

#: ../../../xin/docs/how_to/work_with_microtvm/micro_tflite.ipynb:30047
msgid "Generate TFLite package."
msgstr ""

#: ../../../xin/docs/how_to/work_with_microtvm/micro_tflite.ipynb:30051
msgid "flatc --python schema.fbs"
msgstr ""

#: ../../../xin/docs/how_to/work_with_microtvm/micro_tflite.ipynb:30053
msgid ""
"Add the current folder (which contains generated tflite module) to "
"PYTHONPATH."
msgstr ""

#: ../../../xin/docs/how_to/work_with_microtvm/micro_tflite.ipynb:30057
msgid "export PYTHONPATH=${PYTHONPATH:+$PYTHONPATH:}$(pwd)"
msgstr ""

#: ../../../xin/docs/how_to/work_with_microtvm/micro_tflite.ipynb:30059
msgid ""
"To validate that the TFLite package was installed successfully, ``python "
"-c \"import tflite\"``"
msgstr ""

